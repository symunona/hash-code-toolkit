mmy{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "youtube - just graph",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",                            
                "graph": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - kittens",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"kittens",
                "solvers": "cache-server-endpoint-hybrid"                
            },
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test graph",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"test me_at_the_zoo",
                
                "graph": "true"                
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - parse-all",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",               
                "solvers": "solver1"                               
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - exceptKittens",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube", 
                "input":"test me_at_the_zoo trending_today videos_worth_spreading",              
                "solvers": "all"                               
            },
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test-output-with-one-solver",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube", 
                "input": "test",              
                "solvers": "solver1",
                "output": "solver1",
                "doNotCacheSolutions": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test-whole-pipeline",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"test me_at_the_zoo",
                "solvers": "endpoint-perspective",
                "output": "endpoint-perspective",
                "export": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - optimize ALL",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",                
                "solvers": "ordered-by-request-quantities endpoint-perspective",
                "graph": "true",
                "output": "endpoint-perspective",
                "export": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "router - parser",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "input": "charleston_road rue_de_londres opera lets_go_higher",
                "solvers": "dummy-solver",
                "task": "router",                
                "graph": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "pizza - just graph",
            "program": "${workspaceFolder}/app.js",
            "env":{                            
                "task": "pizza",                
                "graph": "true"                
            }            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "pizza - example small allmagics",
            "program": "${workspaceFolder}/app.js",
            "env":{                
                "solvers": "hut",
                "input": "example small medium",
                "allmagics": "true",
                "task": "pizza",                
                "graph": "true"                
            },
            "console": "integratedTerminal"
        }
        ,
        {
            "type": "node",
            "request": "launch",
            "name": "pizza - all",
            "program": "${workspaceFolder}/app.js",
            "env":{                
                "solvers": "hut",
                "input": "example small medium big",
                "allmagics": "false",
                "task": "pizza",                                
                "output": "hut"
            },
            "console": "integratedTerminal"
        }
        ,
        {
            "type": "node",
            "request": "launch",
            "name": "test mr-math",
            "program": "${workspaceFolder}/mr-math.js",            
        },

        {
            "type": "node",
            "request": "launch",
            "name": "pizza - just output",
            "program": "${workspaceFolder}/app.js",
            "env":{                
                "solvers": "hut",
                "input": "example small",
                "allmagics": "false",
                "task": "pizza",                                
                "output": "hut"                
            },
            "console": "integratedTerminal"
        },


        {
            "type": "node",
            "request": "launch",
            "name": "tesla - parser",
            "program": "${workspaceFolder}/app.js",
            "env":{                                                             
                "task": "tesla",                
                "force": "true"
            },
            "console": "integratedTerminal"
        }


    ]
}