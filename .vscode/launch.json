    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "youtube - just graph",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",                            
                "graph": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - kittens",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"kittens",
                "solvers": "cache-server-endpoint-hybrid"                
            },
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test graph",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"test me_at_the_zoo",
                
                "graph": "true"                
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - parse-all",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",               
                "solvers": "solver1"                               
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - exceptKittens",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube", 
                "input":"test me_at_the_zoo trending_today videos_worth_spreading",              
                "solvers": "all"                               
            },
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test-output-with-one-solver",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube", 
                "input": "test",              
                "solvers": "solver1",
                "output": "solver1",
                "doNotCacheSolutions": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - test-whole-pipeline",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",
                "input":"test me_at_the_zoo",
                "solvers": "endpoint-perspective",
                "output": "endpoint-perspective",
                "export": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "youtube - optimize ALL",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "task": "youtube",                
                "solvers": "ordered-by-request-quantities endpoint-perspective",
                "graph": "true",
                "output": "endpoint-perspective",
                "export": "true"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "router - parser",
            "program": "${workspaceFolder}/app.js",
            "env":{
                "input": "charleston_road",
                "solvers": "dummy-solver",
                "task": "router",
                "force": "true",
                "graph": "true"
            }
        }
        
        
    ]
}